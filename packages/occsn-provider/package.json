    "redux-mock-store": "^1.5.1",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.61.0",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-postcss": "^1.6.2",
    "sass-loader": "^7.1.0"
