    "node-sass": "^4.11.0",
    "occasion-sdk": "git+https://github.com/GetOccasion/occasion-sdk-js.git",
    "poi-plugin-react": "git+https://github.com/nicklandgrebe/poi-plugin-react",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reactstrap": "^5.0.0-beta",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
