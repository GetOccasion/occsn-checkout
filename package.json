{
  "name": "occsn_checkout",
  "version": "0.0.1",
  "description": "An Occasion checkout experience built using React-Redux and Mitragyna",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch"
  },
  "keywords": [
    "occasion",
    "checkout",
    "experience",
    "react",
    "redux",
    "mitragyna",
    "occasion-sdk",
    "active-resource"
  ],
  "author": "Nick Landgrebe, Peak Labs LLC DBA Occasion App",
  "license": "MIT",
  "dependencies": {
    "active-resource": "git+https://github.com/nicklandgrebe/activeresource.js.git#feature/immutable",
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "decimal.js": "^10.0.0",
    "immutable": "^3.8.2",
    "mirror-creator": "^1.1.0",
    "mitragyna": "git+https://github.com/nicklandgrebe/mitragyna.git",
    "moment": "^2.20.1",
    "moment-range": "^3.1.1",
    "moment-timezone": "^0.5.14",
    "occasion-sdk": "git+https://github.com/nicklandgrebe/occasion-sdk-js.git",
    "prop-types": "^15.6.0",
    "q": "^1.5.1",
    "react": "^16.2.0",
    "react-currency-formatter": "^1.1.0",
    "react-dom": "^16.2.0",
    "react-html-parser": "^2.0.2",
    "react-redux": "^5.0.6",
    "reactstrap": "^5.0.0-beta",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.2.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-matchers": "^4.2.0",
    "enzyme-to-json": "^3.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.3.0",
    "jest-cli": "^22.3.0",
    "jest-enzyme": "^4.2.0",
    "jest-plugins": "^2.6.0",
    "jest-plugin-action": "^2.6.0",
    "jest-plugin-context": "^2.6.0",
    "jest-plugin-for-each": "^2.6.0",
    "jest-plugin-it-renders": "^2.6.0",
    "jest-plugin-its": "^2.6.0",
    "jest-plugin-set": "^2.6.0",
    "jest-plugin-unhandled-promise": "^2.6.0",
    "poi": "^10.1.8",
    "poi-preset-react": "^9.2.1",
    "poi-preset-resolve-alias": "^1.0.1",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1"
  },
  "jest": {
    "clearMocks": true,
    "setupFiles": [
      "./spec/jest/setup.js"
    ],
    "setupTestFrameworkScriptFile": "./spec/jest/plugins.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^active-resource$": "active-resource/build/active-resource.js",
      "^mitragyna$": "mitragyna/build/mitragyna.min.js",
      "^occasion-sdk$": "occasion-sdk/build/occasion-sdk.js",
      "^app(.*)$": "<rootDir>/app$1",
      "fixtures(.*)$": "<rootDir>/spec/fixtures$1"
    }
  }
}
