{
  "name": "occsn-checkout",
  "version": "0.0.17",
  "description": "Occasion React components for building your own checkout experience",
  "keywords": [
    "occasion",
    "checkout",
    "experience",
    "react",
    "redux",
    "mitragyna",
    "occasion-sdk",
    "active-resource"
  ],
  "author": "Peak Labs LLC DBA Occasion App",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.2.2",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-optional-chaining": "^7.0.0-beta.3",
    "babel-preset-stage-0": "^6.24.1",
    "bili": "^3.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-matchers": "^4.2.0",
    "enzyme-to-json": "^3.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.3.0",
    "jest-cli": "^22.3.0",
    "jest-enzyme": "^4.2.0",
    "jest-plugin-action": "^2.6.0",
    "jest-plugin-context": "^2.6.0",
    "jest-plugin-for-each": "^2.6.0",
    "jest-plugin-it-renders": "^2.6.0",
    "jest-plugin-its": "^2.6.0",
    "jest-plugin-set": "^2.6.0",
    "jest-plugin-unhandled-promise": "^2.6.0",
    "jest-plugins": "^2.6.0",
    "lerna": "^3.13.1",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.61.0",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-postcss": "^1.6.2",
    "sass-loader": "^7.1.0"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "npm run bootstrap",
    "bootstrap": "lerna bootstrap --no-ci --use-workspaces",
    "clean": "npm run clean:artifacts && npm run clean:packages",
    "clean:artifacts": "lerna run clean --parallel",
    "clean:packages": "lerna clean --yes",
    "build": "lerna run build --concurrency=1 --stream",
    "start": "npm run build && lerna run start",
    "prepare:release": "lerna version --force-publish=*",
    "prepare:prerelease": "npm run prepare:release prerelease -- --preid alpha",
    "publish": "lerna publish --force-publish=*"
  },
  "jest": {
    "clearMocks": true,
    "setupFiles": [
      "./spec/jest/setup.js"
    ],
    "setupTestFrameworkScriptFile": "./spec/jest/plugins.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^active-resource$": "active-resource/build/active-resource.min.js",
      "^mitragyna$": "mitragyna/build/mitragyna.min.js",
      "^occasion-sdk$": "occasion-sdk/build/occasion-sdk.js",
      "^app(.*)$": "<rootDir>/app$1",
      "fixtures(.*)$": "<rootDir>/spec/fixtures$1"
    }
  }
}
