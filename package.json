{
  "name": "occsn_checkout",
  "version": "0.0.1",
  "description": "An Occasion checkout experience built using React-Redux and Mitragyna",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "testw": "jest --watch",
    "testd": "node --inspect-brk node_modules/.bin/jest --runInBand --watch"
  },
  "keywords": [
    "occasion",
    "checkout",
    "experience",
    "react",
    "redux",
    "mitragyna",
    "occasion-sdk",
    "active-resource"
  ],
  "author": "Nick Landgrebe, Peak Labs LLC DBA Occasion App",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "occasion-sdk": "git+https://github.com/nicklandgrebe/occasion-sdk-js.git",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reactstrap": "^5.0.0-beta",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.2.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.3.0",
    "jest-cli": "^22.3.0",
    "poi": "^9.6.13",
    "poi-preset-react": "^9.2.1",
    "poi-preset-resolve-alias": "^1.0.1",
    "react-test-renderer": "^16.2.0"
  },
  "jest": {
    "setupFiles": [
      "./spec/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^active-resource$": "active-resource/build/active-resource.min.js",
      "^occasion-sdk$": "occasion-sdk/build/occasion-sdk.min.js"
    }
  }
}
